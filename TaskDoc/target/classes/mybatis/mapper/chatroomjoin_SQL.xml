<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper과 중복 되지 않도록 네임스페이스 기재 -->
<mapper namespace="chatroomjoin_SQL">
	<select id="chatroomjoinlist" resultType="Integer">
	
		SELECT crcode FROM chatroomjoin WHERE uid=#{uid}

	</select>
	
	<insert id="chatroomjoininsert">

		INSERT INTO chatroomjoin VALUES(#{crcode}, #{uid})

	</insert>
	
	<delete id="chatroomjoindelete">
	
		DELETE FROM chatroomjoin WHERE crcode=#{crcode} AND uid=#{uid}
	
	</delete>
</mapper>
	